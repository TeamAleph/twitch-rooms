{
  "name": "twitch-rooms-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch-server": "nodemon server/index.js",
    "watch-client": "watchify ./src/main.js -o ./public/bundle.js -d -v -t [ babelify ]",
    "dev": "npm run watch-client & npm run watch-server",
