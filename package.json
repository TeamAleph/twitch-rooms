  "dependencies": {
    "babelify": "^6.3.0",
    "body-parser": "^1.13.2",
    "browserify": "^10.0.0",
    "express": "^4.12.3",
    "gl-matrix": "^2.3.1",
    "http": "0.0.0",
    "socket.io": "^1.3.5",
    "three": "^0.84.0",
    "watchify": "^3.2.1"
  },
